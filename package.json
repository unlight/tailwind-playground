{
  "name": "tailwind-playground",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "node node_modules/jest/bin/jest --runInBand --verbose",
    "test:w": "npm run test:r -- --watch",
    "test:cov": "npm run test:r -- --collectCoverage",
    "eslint": "echo noeslint",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "test:ci": "npm run test && npm run build"
  },
  "dependencies": {
    "@iconify/iconify": "^2.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.26",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.4",
    "cssnano": "^5.1.7",
    "cssnano-preset-default": "^5.2.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "request": "^2.88.2",
    "tailwind-float-label": "^3.0.0",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-purge-icons": "^0.8.1",
    "vite-tsconfig-paths": "^3.4.1",
    "watchexec-bin": "^1.0.0"
  }
}
