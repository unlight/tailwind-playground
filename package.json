{
  "name": "tailwind-playground",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "node node_modules/jest/bin/jest --runInBand --verbose",
    "test:w": "npm run test:r -- --watch",
    "test:cov": "npm run test:r -- --collectCoverage",
    "eslint": "echo noeslint",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "test:ci": "npm run test && npm run build"
  },
  "dependencies": {
    "@iconify/iconify": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.116",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "cssnano-preset-default": "^5.2.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "request": "^2.88.2",
    "tailwind-float-label": "^3.0.0",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vite-plugin-purge-icons": "^0.9.1",
    "vite-tsconfig-paths": "^3.5.1",
    "watchexec-bin": "^1.0.0"
  }
}
