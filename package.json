{
  "name": "tailwind-playground",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "node node_modules/jest/bin/jest --runInBand --verbose",
    "test:w": "npm run test:r -- --watch",
    "test:cov": "npm run test:r -- --collectCoverage",
    "eslint": "echo noeslint",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "test:ci": "npm run test && npm run build"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.17",
    "cssnano-preset-default": "^5.1.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "request": "^2.88.2",
    "tailwind-float-label": "^2.1.0",
    "tailwindcss": "^3.0.22",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "vite": "^2.8.1",
    "vite-tsconfig-paths": "^3.4.0",
    "watchexec-bin": "^1.0.0"
  }
}
